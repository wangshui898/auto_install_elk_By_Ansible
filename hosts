[logstash]
192.168.2.111 host_name=logstash-1 ansible_ssh_user=root ansible_ssh_port=22 ansible_ssh_pass=......
192.168.2.112 host_name=logstash-2 ansible_ssh_user=root ansible_ssh_port=22 ansible_ssh_pass=......

[kibana]
192.168.2.121 host_name=kibana-1 ansible_ssh_user=root ansible_ssh_port=22 ansible_ssh_pass=......
192.168.2.122 host_name=kibana-2 ansible_ssh_user=root ansible_ssh_port=22 ansible_ssh_pass=......

[mq]
192.168.2.111 host_name=mq1 myid=0 ansible_ssh_user=root ansible_ssh_port=22 ansible_ssh_pass=......
192.168.2.112 host_name=mq2 myid=1 ansible_ssh_user=root ansible_ssh_port=22 ansible_ssh_pass=......
192.168.2.113 host_name=mq3 myid=2 ansible_ssh_user=root ansible_ssh_port=22 ansible_ssh_pass=......

[es]
192.168.2.121 host_name=es-1 node_name=node-1 ansible_ssh_user=root ansible_ssh_port=22 ansible_ssh_pass=......
192.168.2.122 host_name=es-2 node_name=node-2 ansible_ssh_user=root ansible_ssh_port=22 ansible_ssh_pass=......
192.168.2.123 host_name=es-3 node_name=node-3 ansible_ssh_user=root ansible_ssh_port=22 ansible_ssh_pass=......

[HA]
192.168.2.101 host_name=LB-M lb_name=LB-M ansible_ssh_user=root ansible_ssh_port=22 ansible_ssh_pass=......
192.168.2.102 host_name=LB-S lb_name=LB-S ansible_ssh_user=root ansible_ssh_port=22 ansible_ssh_pass=......

## 分组设置,如果主机组部署多个服务,不需要重复填写,填写用来命名的主机组即可
